{
    "name": "pekhota/nova-hugerte",
    "description": "Nova HugeRTE â€“ a self-hosted TinyMCE-compatible editor field",
    "keywords": [
        "laravel",
        "nova",
        "editor",
        "tinymce",
        "hugerte"
    ],
    "suggest": {
        "laravel/nova": "Required to use this field"
    },
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/nova": "^5.0",
        "laravel/nova-devtool": "^1.7"
    },
    "repositories": [
        { "type": "composer", "url": "https://nova.laravel.com" }
    ],
    "autoload": {
        "psr-4": {
            "Pekhota\\NovaHugeRTE\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pekhota\\NovaHugeRTE\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
